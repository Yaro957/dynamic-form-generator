            {
              "name": "amount",
              "label": "Payment Amount",
              "type": "number",
              "required": true
            },
            {
              "name": "date",
              "label": "Payment Date",
              "type": "text",
              "required": true
            },
            {
              "name": "status",
              "label": "Status",
              "type": "text",
              "required": true
